{
  "name": "emart",
  "version": "1.0.0",
  "description": "MERN E-Commerce App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm run dev --prefix frontend",
